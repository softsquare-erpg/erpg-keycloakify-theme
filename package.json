{
    "name": "erpg-keycloakify-theme",
    "version": "1.0.0",
    "description": "Theme for ERPG using Keycloakify. Provides a custom Keycloak theme with modern UI, Bootstrap 5 styling, and seamless integration for the ERPG platform.",
    "author": "Adsadawut h. <l3omb.io>",
    "repository": {
        "type": "git",
        "url": "git://github.com/softsquare-erpg/erpg-keycloakify-theme.git"
    },
    "publishConfig": {
        "registry": "https://maven.pkg.github.com/softsquare-erpg/erpg-keycloak-theme"
    },
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "build-keycloak-theme": "npm run build && keycloakify build",
        "storybook": "storybook dev -p 6006",
        "format": "prettier . --write",
        "test-inside": "keycloakify start-keycloak"
    },
    "license": "MIT",
    "keywords": [],
    "dependencies": {
        "bootstrap": "^5.3.6",
        "bootstrap-icons": "^1.13.1",
        "keycloakify": "^11.8.32",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.15.0",
        "@storybook/react": "^8.1.10",
        "@storybook/react-vite": "^8.1.10",
        "@types/bootstrap": "^5",
        "@types/react": "^18.2.43",
        "@types/react-dom": "^18.2.17",
        "@typescript-eslint/eslint-plugin": "^8.15.0",
        "@typescript-eslint/parser": "^8.15.0",
        "@vitejs/plugin-react": "^4.2.1",
        "eslint": "^9.15.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-plugin-react-refresh": "^0.4.14",
        "eslint-plugin-storybook": "^0.11.1",
        "globals": "^15.12.0",
        "prettier": "3.3.1",
        "storybook": "^8.1.10",
        "typescript": "^5.2.2",
        "typescript-eslint": "^8.15.0",
        "vite": "^5.0.8"
    },
    "engines": {
        "node": "^18.0.0 || >=20.0.0"
    },
    "packageManager": "yarn@4.5.1"
}
